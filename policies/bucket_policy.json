{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowLambdaToPutObject",
            "Effect": "Allow",
            "Principal": {
                "AWS": "you-aws-account-no.:root"
            },
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Resource": "arn:aws:s3:::my-backup-archive-destination-1234/*"
        }
    ]
}